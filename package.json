    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "apple-signin-auth": "^1.7.6",
    "aws-xray-sdk": "^3.6.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "google-auth-library": "^8.9.0",
    "handlebars": "^4.7.8",
    "mime-types": "^2.1.35",
    "ms": "^2.1.3",
    "nestjs-i18n": "^10.4.5",
    "nodemailer": "^6.9.13",
    "passport": "^0.6.0",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.1",
